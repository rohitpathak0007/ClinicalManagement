@{
    Layout = "~/Views/Shared/Customer_Layout.cshtml";
}



@model Model.CustomerModel

<script type="text/javascript">
    $(document).ready(function () {
        $('#btnBack').click(function () {
            debugger;
           
            $('#frmHome').submit();
        });
    });
    
</script>
<form method="post" action="~/Home/Home" id="frmHome"> </form>
<div>
    <div class="fl col-md-12">
        <div class="portlet-body">

            <h3 class="page-title">View Patient's</h3>
        </div>
    </div>
    <div class="clearfix"></div>
</div>
<br />
<div class="clearfix"></div>

<div class="row">
    <div class="col-sm-12">
        <div id="dvSccessFail">
            @Html.Partial("_SuccessFail")
        </div>
        <div class="panel panel-default">
            <div class="panel-body">
                <div id="" class="row clearfix">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Appointment ID</label>
                            @Html.TextBoxFor(m => m.AppointmentID, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">First Name</label>
                            @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Last Name</label>
                            @Html.TextBoxFor(m => m.LastName, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Gender</label>
                            @Html.TextBoxFor(m => m.Gender, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Age</label>
                            @Html.TextBoxFor(m => m.Age, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Time</label>
                            @Html.TextBoxFor(m => m.AppDateTime, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                     <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Email ID</label>
                            @Html.TextBoxFor(m => m.EmailId, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                     <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Contact No</label>
                            @Html.TextBoxFor(m => m.PhoneNumber, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                     <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Descriptions</label>
                            @Html.TextBoxFor(m => m.Descriptions, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                     <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Doctor</label>
                            @*@Html.TextBoxFor(m => m.Doctor., new { @class = "form-control", @readonly = "true" })*@
                            @Html.DropDownListFor(m => m.DoctorID,  ViewBag.Doctor as SelectList, new { @class = "form-control", @readonly = "true"})
                        </div>
                    </div>
                </div>
                  <br />
                    <table>
                        <tr>
                            <td>
                                <a id="btnBack"  class="btn btn-skin btn-block">Back</a>
                            </td>
                        </tr>
                    </table>
            </div>
        </div>
    </div>
</div>
