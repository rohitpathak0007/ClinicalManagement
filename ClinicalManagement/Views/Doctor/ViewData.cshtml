@{
    Layout = "~/Views/Shared/Customer_Layout.cshtml";
}



@model Model.DoctorModel

<script type="text/javascript">
    $(document).ready(function () {
        $('#btnBack').click(function () {
            debugger;

            $('#frmHome').submit();
        });
    });

</script>
<form method="post" action="~/Doctor/Index" id="frmHome"></form>
<div>
    <div class="fl col-md-12">
        <div class="portlet-body">

            <h3 class="page-title">View Doctor</h3>
        </div>
    </div>
    <div class="clearfix"></div>
</div>
<br />
<div class="clearfix"></div>
<div class="row">
    <div class="col-sm-12">
        <div id="dvSccessFail">
            @Html.Partial("_SuccessFail")
        </div>
        <div class="panel panel-default">
            <div class="panel-body">
                <div id="" class="row clearfix">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Doctor ID</label>
                            @Html.TextBoxFor(m => m.DoctorID, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">First Name</label>
                            @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Last Name</label>
                            @Html.TextBoxFor(m => m.LastName, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Gender</label>
                            @Html.TextBoxFor(m => m.Gender, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Age</label>
                            @Html.TextBoxFor(m => m.Age, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Address</label>
                            @Html.TextAreaFor(m => m.Address, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Email ID</label>
                            @Html.TextBoxFor(m => m.EmailId, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Contact No</label>
                            @Html.TextBoxFor(m => m.PhoneNumber, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Descriptions</label>
                            @Html.TextBoxFor(m => m.AboutDoctor, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">From Time</label>
                            @Html.TextBoxFor(m => m.FrmTime, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>

                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">To Time</label>
                            @Html.TextBoxFor(m => m.ToTime, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">User Name</label>
                            @Html.TextBoxFor(m => m.Username, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">User Code</label>
                            @Html.TextBoxFor(m => m.UserCode, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="required">Degree</label>
                            @Html.TextBoxFor(m => m.Degree, new { @class = "form-control", @readonly = "true" })
                        </div>
                    </div>
                </div>
                <br />
                <table>
                    <tr>
                        <td>
                            <a id="btnBack" class="btn btn-skin btn-block">Back</a>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
</div>
